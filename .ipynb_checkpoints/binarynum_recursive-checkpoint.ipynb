   "source": [
    "## 재귀함수를 이용해 이진수 출력"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "2c722ad0",
   "metadata": {},
   "outputs": [],
   "source": [
    "lst = []\n",
    "def solution(n):\n",
    "    \n",
    "    if n == 0:\n",
    "        return \n",
    "    else:\n",
    "        solution(n//2)\n",
    "        lst.append(n%2)\n",
    "    return int(''.join(map(str, lst)))"
   ]
  },
